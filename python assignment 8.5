#python data structure
fh = raw_input("Unesite tekst\n")
tekst = open(fh)
zbroj = 0
for linija in tekst:
    if linija.startswith("From "):
        rijeci = linija.rstrip().split()
        email = rijeci[1]
        print(email)
        zbroj +=1
    else:
        continue

print("There were", zbroj, "lines in the file with From as the first word")
